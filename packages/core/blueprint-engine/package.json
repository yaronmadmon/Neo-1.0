{
  "name": "@neo/blueprint-engine",
  "version": "1.0.0",
  "description": "Blueprint Engine - Single source of truth for app generation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./tokens": {
      "import": "./src/tokens/index.ts",
      "types": "./src/tokens/index.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@neo/contracts": "*",
    "@neo/integrations": "*",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "rimraf": "^5.0.5"
  }
}
